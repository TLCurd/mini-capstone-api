@products.map do |product|
  {
    id: product.id,
    name: product.name,
    price: product.price,
    image_url: product.image_url,
    description: product.description,
    supplier_id: product.supplier_id,
    created_at: product.better_created_at,
    updated_at: product.better_updated_at
  }
end
